{
  "manifest_version": 3,
  "name": "BA Paywall Smasher",
  "version": "0.0.1",
  "description": "Gets around that pesty paywall shhhh",
  "icons": {
    "48": "icons/icon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.bonappetit.com/",
        "*://bonappetit.com/",
        "*://*/bonappetit.com/"
      ],
      "js": ["index.js"]
    }
  ]
}
